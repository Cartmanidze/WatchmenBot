{
  "Telegram": {
    "BotToken": "PUT_YOUR_TELEGRAM_BOT_TOKEN_HERE",
    "WebhookUrl": "https://your-domain.com/telegram/update",
    "WebhookSecret": "GENERATE_RANDOM_SECRET_256_CHARS",
    "ValidateIpRange": true,
    "DeleteWebhookOnShutdown": false
  },
  "Llm": {
    "DefaultProvider": "openrouter",
    "Providers": [
      {
        "Name": "openrouter",
        "Type": "openrouter",
        "ApiKey": "sk-or-v1-your-openrouter-api-key-here",
        "BaseUrl": "https://openrouter.ai/api/v1",
        "Model": "deepseek/deepseek-chat",
        "Enabled": true,
        "Priority": 1,
        "Tags": ["default", "cheap"]
      },
      {
        "Name": "openrouter-uncensored",
        "Type": "openrouter",
        "ApiKey": "sk-or-v1-your-openrouter-api-key-here",
        "BaseUrl": "https://openrouter.ai/api/v1",
        "Model": "mistralai/mistral-small",
        "Enabled": false,
        "Priority": 2,
        "Tags": ["uncensored"]
      }
    ]
  },
  "Embeddings": {
    "ApiKey": "sk-your-openai-api-key-here",
    "BaseUrl": "https://api.openai.com/v1",
    "Model": "text-embedding-3-small",
    "Dimensions": 1536,
    "BackgroundIndexing": {
      "Enabled": true,
      "BatchSize": 50,
      "DelayBetweenBatchesSeconds": 30,
      "MaxBatchesPerRun": 100,
      "IntervalMinutes": 5
    }
  },
  "Database": {
    "ConnectionString": "Host=localhost;Database=watchmenbot;Username=postgres;Password=your_password"
  },
  "DailySummary": {
    "TimeOfDay": "21:00"
  },
  "Admin": {
    "UserId": 0,
    "Username": "gleb_bezrukov",
    "ReportTime": "10:00",
    "TimezoneOffset": "+06:00"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "System.Net.Http.HttpClient": "Warning",
      "System.Net.Http.HttpClient.EmbeddingClient": "Warning",
      "System.Net.Http.HttpClient.OpenRouterClient": "Warning"
    }
  },
  "AllowedHosts": "*"
}
